<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    {{ include('headIncludes.html.twig') }}
    {% block head %}

    {% endblock %}
</head>
<body class="hold-transition naved-body">

{{ include('navbar.html.twig') }}

    {% if app.request.hasPreviousSession %}
        {% for type, messages in app.session.flashbag.all() %}
            {% for message in messages %}
                <div class="alert alert-{{ type }}">
                    {{ message }}
                </div>
            {% endfor %}
        {% endfor %}
    {% endif %}

    <div class="container">
        <div class="table-wrapper">
            {% block body %}
                {% block site_content %}

                {% endblock site_content %}
            {% endblock %}
        </div>
    </div>
    {% if path('carrier_index') == app.request.requestUri %}
        <script src="{{ asset('build/js/table-filter.js') }}"></script>
    {% endif %}
    </body>
</html>
